{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": ["astro/client"],
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@components/*": ["src/components/*"]
    }
  }
}
